{
	"_version": "1.8.0",
	"sap.app": {
		"id": "com.techedge.training.SAPUI5Training",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"GWSAMPLE_BASIC": {
				"uri": "/ES5/sap/opu/odata/iwbep/GWSAMPLE_BASIC/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.techedge.training.SAPUI5Training.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.techedge.training.SAPUI5Training.i18n.i18n"
				}
			},
			"": {
				"uri": "/ES5/sap/opu/odata/iwbep/GWSAMPLE_BASIC/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "GWSAMPLE_BASIC",
				"preload": true
			},
			"filters": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/filters.json",
				"preload": false
			},
			"data": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/data.json",
				"preload": false
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.techedge.training.SAPUI5Training.view",
				"controlId": "rootControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "TargetBusinessPartnerList",
				"pattern": "",
				"target": ["TargeteBusinessPartnerList"]
			}, {
				"name": "TargetBusinessPartnerDetail",
				"pattern": "partner/:BusinessPartnerID:",
				"target": ["TargeteBusinessPartnerList", "TargetBusinessPartnerDetail"]
			}],
			"targets": {
				"TargeteBusinessPartnerList": {
					"viewType": "XML",
					"controlAggregation": "masterPages",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "BusinessPartnerList"
				}, 
				"TargetBusinessPartnerDetail": {
					"viewType": "XML",
					"controlAggregation": "detailPages",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "BusinessPartnerDetail"
				}
			}
		}
	}
}